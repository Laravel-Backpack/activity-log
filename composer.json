{
    "name": "backpack/activity-log",
    "description": "Activity Log for Backpack",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Almeida",
            "email": "promatik@gmail.com",
            "homepage": "https://promatik.pt"
        },
        {
            "name": "Cristian Tabacitu",
            "email": "cristian.tabacitu@backpackforlaravel.com",
            "homepage": "https://backpackforlaravel.com"
        }
    ],
    "homepage": "https://github.com/backpack/activity-log",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "ActivityLog"
    ],
    "require": {
        "backpack/crud": "^6.0",
        "spatie/laravel-activitylog": "^4.7",
        "laravel/prompts": "^0.1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0",
        "orchestra/testbench": "~8"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\ActivityLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\ActivityLog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backpack\\ActivityLog\\ActivityLogServiceProvider"
            ]
        }
    }
}
